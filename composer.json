{
	"name": "surda/webloader-jm",
	"description": "Tool for loading or deploying CSS and JS files into web pages",
	"keywords": ["webloader", "css", "javascript", "assets", "nette"],
	"license": "MIT",
	"authors": [
		{
			"name": "Jan Marek",
			"email": "mail@janmarek.net"
		},
		{
			"name": "Pavel Linhart",
			"homepage": "https://github.com/Gappa"
		},
		{
			"name": "Radek Šuráni",
			"homepage": "http://surani.cz",
			"email": "radek@surani.cz"
		}
	],
	"require": {
		"php": ">= 7.1",
		"nette/application": "^3.0",
		"nette/di": "^3.0",
		"nette/utils": "^3.0",
		"ext-json": "*"
    },
	"require-dev": {
		"nette/application": "^3.0",
		"nette/bootstrap": "^3.0",
		"nette/caching": "^3.0",
		"nette/component-model": "^3.0",
		"nette/database": "^3.0",
		"nette/deprecated": "^2.3",
		"nette/di": "^3.0",
		"nette/finder": "^2.5",
		"nette/forms": "^3.0",
		"nette/http": "^3.0",
		"nette/mail": "^3.0",
		"nette/neon": "^3.0",
		"nette/php-generator": "^3.0",
		"nette/reflection": "^2.4",
		"nette/robot-loader": "^3.0",
		"nette/safe-stream": "^2.4",
		"nette/security": "^3.0",
		"nette/tokenizer": "^3.0",
		"nette/utils": "^3.0",
		"latte/latte": "^2.5",
		"tracy/tracy": "^2.6",
		"oyejorge/less.php": "^1.7",
		"leafo/scssphp": "^0.7",
		"kylekatarnls/coffeescript": "1.3.*",
		"mockery/mockery": "1.*",
		"phpunit/phpunit": "7.*",
		"jakub-onderka/php-parallel-lint": "~0.7",
		"phpstan/phpstan-shim": "^0.11.0",
		"phpstan/phpstan-nette": "^0.11.0"
	},
	"autoload": {
		"psr-4": {
			"WebLoader\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"WebLoader\\Test\\": "tests/"
		}
	},
	"scripts": {
		"tests": [
			"vendor/bin/tester -s -p php -C tests"
		],
		"phpstan": [
			"vendor/bin/phpstan analyse -l max -c phpstan.neon src"
		],
		"da": [
			"composer dump-autoload -o"
		]
	},
	"minimum-stability": "stable"
}
